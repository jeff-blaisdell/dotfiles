[alias]
    c = commit -am
    up = pull
    p = push
    s = status -s
    df = diff --color --color-words --abbrev
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[color]
    ui = always
[core]
    excludesfile = ~/.gitignore
    attributesfile = ~/.gitattributes
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
    log = true
[url "git@github.com:"]
    insteadOf = "gh:"
    pushInsteadOf = "github:"
    pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
    insteadOf = "github:"
[url "git@gist.github.com:"]
    insteadOf = "gst:"
    pushInsteadOf = "gist:"
    pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
    insteadOf = "gist:"
[alias]
    br = branch
    caa = commit -a --amend
    cd = checkout develop
    co = checkout
    df = diff --relative --name-only upstream/develop
    dtud = difftool upstream/develop..HEAD --
    du = diff --relative upstream/develop
    fa = fetch --all
    ffp = flow feature publish
    ffs = flow feature start
    l = log --relative --graph
    ls = log --relative --graph --stat
    pf = "!if [ `git rev-parse --abbrev-ref --symbolic-full-name '@{u}' |sed -e 's#/.*##'` != 'upstream' ]; then git push -f; else echo 'DO NOT PUSH FORCE TO UPSTREAM'; fi"
    prud = pull --rebase upstream develop
    riud = rebase -i upstream/develop
    s = status
    b = branch -vv
