[user]
    name = Jeff Blaisdell
    email = jeff.blaisdell@gmail.com
[push]
    default = simple
[merge]
    tool = vimdiff
[diff]
    tool = vimdiff
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    br = branch
    caa = commit -a --amend
    cd = checkout develop
    co = checkout
    df = diff --relative --name-only upstream/develop
    dtud = difftool upstream/develop..HEAD --
    du = diff --relative upstream/develop
    fa = fetch --all
    ffp = flow feature publish
    ffs = flow feature start
    l = log --relative --graph
    ls = log --relative --graph --stat
    pf = "!if [ `git rev-parse --abbrev-ref --symbolic-full-name '@{u}' |sed -e 's#/.*##'` != 'upstream' ]; then git push -f; else echo 'DO NOT PUSH FORCE TO UPSTREAM'; fi"
    prud = pull --rebase upstream develop
    riud = rebase -i upstream/develop
    s = status
    b = branch -vv
